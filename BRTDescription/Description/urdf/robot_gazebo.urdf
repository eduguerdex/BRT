        <inertia ixx="19.69" ixy="2.25" ixz="0.40" iyy="6.09" iyz="-1.97" izz="19.58" />
        </inertial>
    </link>
    <gazebo reference="1_link">
        <material>Gazebo/White</material>
    </gazebo>

    <joint name="1-2_joint" type="revolute">
        <parent link="1_link"/>
